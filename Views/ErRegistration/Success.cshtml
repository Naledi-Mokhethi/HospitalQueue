@model HospitalQueue.Models.ErRegistration

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ER Registration</title>
    <!-- mobile responsive meta -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <!-- Slick Carousel -->
    <link rel="stylesheet" href="~/plugins/slick/slick.css">
    <link rel="stylesheet" href="~/plugins/slick/slick-theme.css">
    <!-- FancyBox -->
    <link rel="stylesheet" href="~/plugins/fancybox/jquery.fancybox.min.css"> 

    <!-- Stylesheets -->
    <link href="~/css/style.css" rel="stylesheet">

    <!--Favicon-->
    <link rel="shortcut icon" href="~/favicon.ico" type="image/x-icon">

</head>
<body>
    <header>
        <!--Header Upper-->
        <section class="header-uper">
            <div class="container clearfix">
                <div class="logo">
                    <figure>
                        <a asp-area="" asp-controller="Home" asp-action="Index"><img src="~/images/logo.png" alt="" width="130"></a>
                    </figure>
                </div>
                <div class="right-side">
                    <ul class="contact-info">
                        <li class="item">
                            <strong>Email</strong>
                            <br>
                                <span>info@medic.com</span>
                        </li>
                        <li class="item">
                            <strong>Call Now</strong>
                            <br>
                            <span>+(27) - 112</span>
                        </li>
                    </ul>
                </div>
            </div>
        </section>
    </header>
<div>
    <h2>Registration submitted, you may close the page</h2>
</div>

</body>
</html>
